{
  "name": "baggage",
  "version": "0.0.6",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host app.citypassenger.com --port 4200",
    "start:fr": "ng serve --configuration=fr --host app.citypassenger.com --port 4200",
    "start:en": "ng serve --configuration=en --host app.citypassenger.com --port 4200",
    "start:fr:proxy": "ng serve --configuration=fr --public-host app.citypassenger.com --port 4200 --proxy-config src/proxy.conf.json",
    "start:en:proxy": "ng serve --configuration=en --public-host app.citypassenger.com --port 4200 --proxy-config src/proxy.conf.json",
    "build": "ng build",
    "build:fr": "ng build --configuration=fr",
    "build:en": "ng build --configuration=en",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "int:extract": "ng extract-i18n --output-path src/multilingual --outFile translation.xlf"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.2.7",
    "@angular/cdk": "^11.2.5",
    "@angular/common": "^11.2.7",
    "@angular/compiler": "^11.2.7",
    "@angular/core": "^11.2.7",
    "@angular/forms": "^11.2.7",
    "@angular/material": "^11.2.5",
    "@angular/material-moment-adapter": "^11.2.5",
    "@angular/platform-browser": "^11.2.7",
    "@angular/platform-browser-dynamic": "^11.2.7",
    "@angular/router": "^11.2.7",
    "@mat-datetimepicker/core": "^6.0.0",
    "chart.js": "^2.9.4",
    "moment": "^2.29.1",
    "rxjs": "~6.6.6",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.4",
    "@angular/cli": "^11.2.5",
    "@angular/compiler-cli": "^11.2.7",
    "@angular/localize": "^11.2.7",
    "@types/d3": "^5.16.4",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.19.9",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "^4.0.5"
  }
}
